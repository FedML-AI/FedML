{
  "name": "@fedml/spider",
  "type": "module",
  "version": "0.0.1-beta-3",
  "description": "Collaborative Learning from Scattered Data on Browser",
  "author": "fedml-inc",
  "license": "ISC",
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/FedML-AI/FedML#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FedML-AI/FedML.git"
  },
  "bugs": {
    "url": "https://github.com/FedML-AI/FedML/issues"
  },
  "keywords": [
    "Collaborative",
    "AI",
    "MLOps"
  ],
  "exports": {
    ".": {
      "types": {
        "require": "./lib/index.d.ts",
        "default": "./lib/index.d.ts"
      },
      "browser": {
        "require": "./dist/browser/FedmlSpider.cjs",
        "default": "./lib/index.js"
      },
      "default": {
        "require": "./dist/node/FedmlSpider.cjs",
        "default": "./lib/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "jsdelivr": "dist/FedmlSpider.min.js",
  "unpkg": "dist/FedmlSpider.min.js",
  "main": "lib/index.js",
  "module": "dist/esm/FedmlSpider.mjs",
  "types": "lib/index.d.ts",
  "files": [
    "dist",
    "lib",
    "*.d.ts",
    "README.md"
  ],
  "scripts": {
    "build": "tsc & eslint lib --fix & rollup -c",
    "dev": "tsc -w & rollup -c --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "npm run build",
    "release": "bumpp --commit --push --tag && npm publish --access=public",
    "start": "esno src/index.ts",
    "typecheck": "tsc --noEmit",
    "test": "vitest"
  },
  "peerDependencies": {
    "@tensorflow/tfjs": "^4.4.0",
    "@tensorflow/tfjs-vis": "^1.1.0",
    "aws-sdk": "^2.1359.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.309.0",
    "core-js": "^3.29.1",
    "mqtt": "^4.3.7",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.32.0",
    "@babel/preset-env": "^7.21.4",
    "@rollup/plugin-alias": "^4.0.3",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^23.0.7",
    "@rollup/plugin-json": "^5.0.2",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.4.1",
    "@types/node": "^18.15.11",
    "@types/uuid": "^9.0.1",
    "aws-sdk": "^2.1359.0",
    "bumpp": "^8.2.1",
    "eslint": "^8.37.0",
    "esno": "^0.16.3",
    "rollup": "^3.20.2",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-bundle-size": "^1.0.3",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "rollup-plugin-polyfill-node": "^0.12.0",
    "typescript": "^5.0.2",
    "vite": "^3.2.5",
    "vitest": "^0.25.8"
  }
}
